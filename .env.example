# .env.example
# Copy this file to .env and update values as needed

# Domain and username for NdustrOS
NDUSTROS_USER=TTUSA\NDUSTROS
# Password for the above user
NDUSTROS_PASS=your_password_here

# Local Access database for extraction
ACCESS_DB_PATH=Opnordrp-vlad-copy.accdb
# ODBC driver used for the Access database
ACCESS_DB_DRIVER={Microsoft Access Driver (*.mdb, *.accdb)}

# NdustrOS connection defaults
NDUSTROS_SERVER=PLATSRVR           # Database server host
NDUSTROS_PORT=1583                 # Server port
NDUSTROS_DB=NdustrOS               # Database name
NDUSTROS_DRIVER=Pervasive ODBC Client Interface  # ODBC driver

# Set to 'true' to use the local SQLite DB instead of NdustrOS
USE_SQLITE=false
